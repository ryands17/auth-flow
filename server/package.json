{
  "name": "@auth-flow/server",
  "version": "1.0.0",
  "private": true,
  "description": "Express server with Prisma",
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --transpile-only ./src",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "prisma": "dotenv -- prisma",
    "db:create": "yarn prisma migrate save --experimental",
    "db:apply": "yarn prisma migrate up --experimental",
    "seed": "dotenv -- ts-node prisma/seed.ts",
    "lint": "eslint --cache --fix"
  },
  "keywords": [],
  "author": {
    "name": "Ryan Dsouza",
    "email": "dsouza.ryan.11@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.4.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.4.1",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7",
    "typesync": "^0.7.0"
  }
}
