{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "description": "Fastify server with Prisma",
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --transpile-only ./src",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "prisma": "dotenv -- prisma",
    "db:create": "yarn prisma migrate save --experimental",
    "db:apply": "yarn prisma migrate up --experimental",
    "seed": "dotenv -- ts-node prisma/seed.ts",
    "lint": "eslint --cache --fix",
    "test": "dotenv -e .env.test -- jest"
  },
  "keywords": [],
  "author": {
    "name": "Ryan Dsouza",
    "email": "dsouza.ryan.11@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.9.0",
    "bcrypt": "^5.0.0",
    "fastify": "^3.6.0",
    "fastify-cookie": "^4.1.0",
    "fastify-cors": "^4.1.0",
    "fastify-helmet": "^5.0.3",
    "jsonwebtoken": "^8.5.1",
    "zod": "^2.0.0-beta.14"
  },
  "devDependencies": {
    "@prisma/cli": "^2.9.0",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/eslint": "^7.2.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "jest": "^26.6.0",
    "jest-environment-node": "^26.6.0",
    "nanoid": "^3.1.13",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3",
    "typesync": "^0.7.0"
  }
}
